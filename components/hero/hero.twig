<div class="hero">
  <div class="hero__media">
    {% block media %}{% endblock %}
  </div>
  <div class="hero__content">
    {% block content %}{% endblock %}
    <div class="hero__cta">
      {% for cta in ctas %}
        <a href="{{ cta.url }}">{{ cta.title }}</a>
      {% endfor %}
    </div>
  </div>
</div>
